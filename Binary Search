int n =potions.size();
      vector<int>ans;
      for(int spell:spells){
        int count =0;
        vector<int>temp=potions;
        for(int i =0;i<n;i+=1){
            temp[i]=temp[i]*spell;
        }
        for(int t:temp){
            if(t>=success){
                count=count+1;
            }
        }
        ans.push_back(count);
      } 
      return ans;
    }
